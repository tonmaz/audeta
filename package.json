{
  "name": "audeta",
  "author": "@tonde",
  "license": "private",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start:tunnel": "ngrok http 8888",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "packages:upgrade": "yarn upgrade-interactive --latest",
    "analyze": "cross-env ANALYZE=true yarn build",
    "analyse:unused": "next-unused",
    "svg": "npx svgr -d src/components/svg src/svg --ext tsx --template src/utils/svg/svgTemplate.ts",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "next-unused": {
    "alias": {},
    "debug": true,
    "include": [
      "src"
    ],
    "exclude": [
      ".test.",
      "src/utils/extend-jest/*",
      "src/utils/svg/svgTemplate.ts"
    ],
    "entrypoints": [
      "src/pages"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.1.1",
    "@emotion/styled": "11.0.0",
    "@mdx-js/loader": "1.6.21",
    "@next/mdx": "10.0.3",
    "@reduxjs/toolkit": "^1.5.0",
    "@zeit/next-source-maps": "^0.0.3",
    "bootstrap": "^5.0.0-beta1",
    "bootstrap-icons": "^1.1.0",
    "cross-env": "^6.0.3",
    "next": "latest",
    "next-translate": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "reactstrap": "^8.7.1",
    "redux-injectors": "^1.3.0",
    "redux-saga": "^1.1.3",
    "sass": "^1.30.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.0.0",
    "@next/bundle-analyzer": "9.5.3",
    "@next/mdx": "^10.0.3",
    "@svgr/cli": "5.4.0",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.0.10",
    "next-unused": "0.0.3",
    "prettier": "^2.0.2",
    "typescript": "^4.1.2"
  }
}
